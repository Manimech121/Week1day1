package week1.day1;



public class PrimeNumbers {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
       int chkNum = 13;
       String x = "Prime";
       String y = "not a prime";
       boolean chkVariable = false;
       for (int i = 2; i < chkNum; i++) {
		int chkInp = (chkNum %i);
	if (chkInp == 0) {
       chkVariable = true;
	}
     	
	}
       if (chkVariable){
    		   System.out.println(x);
       } else {
       
       System.out.println(y);
       }  
	}

}
